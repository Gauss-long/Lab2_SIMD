{
    "version": 4,
    "configurations": [
      {
        "name": "Linux ARM64 (NEON)",
        "compilerPath": "/usr/bin/g++",
        "cStandard": "c11",
        "cppStandard": "c++14",
        "intelliSenseMode": "linux-gcc-arm64",
        "defines": [
          "__ARM_NEON__",
          "__aarch64__"
        ],
        "includePath": [
